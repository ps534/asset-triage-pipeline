# OpenAI API Key for LLM calls and web search
OPENAI_API_KEY=your_api_key_here

# Optional: LiteLLM configuration
LITELLM_LOG=INFO

# Optional: LiteLLM Proxy URL (if using a proxy server)
LITELLM_PROXY_URL="https://llmgateway.experiment.trialspark.com/"

# Optional: Disable SSL verification for proxy (development only)
LITELLM_DISABLE_SSL_VERIFY=true

# Optional: Model configuration for each prompt
# PROMPT_A_MODEL=gpt-4o-mini    # Repurposing generation (default: gpt-4o-mini)
# PROMPT_C_MODEL=gpt-4o         # Asset screening (default: gpt-4o)